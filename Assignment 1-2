#include <iostream>

using namespace std;

int main()
{
    cout <<"enter the lenth of numbers being added";
    int x;
    cin>>x;

    int a;
    int  sum =0;

    for(int i=1 ; i<=x ;i++)
    {
        cout <<"enter #"<<i <<endl;
        cin>>a;
        sum=sum+a;
    }
    cout <<"the sum "<< sum;
}
